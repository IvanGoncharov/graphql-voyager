{
  "name": "graphql-voyager",
  "version": "1.1.0",
  "description": "GraphQL introspection viewer",
  "main": "dist/voyager.lib.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config build/webpack.js --hot --mode=development",
    "clean:release": "rimraf dist middleware",
    "bundle:standalone": "webpack --mode=production --config build/webpack.js",
    "bundle:lib": "webpack --env lib --mode=production --config build/webpack.js",
    "bundle": "npm run bundle:lib && npm run bundle:standalone",
    "compile:middleware": "tsc -d src/middleware/index.ts --outDir middleware --lib ES6,DOM,esnext.asynciterable",
    "build:release": "npm run bundle && npm run compile:middleware && npm run declarations",
    "deploy": "deploy-to-gh-pages --local --update demo-dist",
    "stats": "NODE_ENV=production webpack --json --mode=production --config build/webpack.js > stats.json",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "declarations": "tsc --emitDeclarationOnly -p tsconfig.lib.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-arrow-functions": "^7.16.7",
    "@babel/plugin-transform-block-scoping": "^7.16.7",
    "@babel/plugin-transform-classes": "^7.16.7",
    "@babel/plugin-transform-destructuring": "^7.17.3",
    "@types/classnames": "^2.3.1",
    "@types/clipboard": "^2.0.7",
    "@types/commonmark": "^0.27.5",
    "@types/lodash": "^4.14.179",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "deploy-to-gh-pages": "1.3.7",
    "file-loader": "^6.2.0",
    "graphql": "^16.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.8",
    "postcss-cssnext": "^3.1.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-variables-loader": "^6.0.0",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-clipboard.js": "2.0.16",
    "react-dom": "^17.0.2",
    "react-svg-loader": "^3.0.3",
    "rimraf": "3.0.2",
    "style-loader": "^3.3.1",
    "svg-as-symbol-loader": "^1.2.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "graphql": ">=15.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@f/animate": "^1.0.1",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.10",
    "commonmark": "^0.30.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "svg-pan-zoom": "^3.6.1",
    "viz.js": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/APIs-guru/graphql-voyager.git"
  }
}
