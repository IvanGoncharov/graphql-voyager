{
  "name": "graphql-voyager",
  "version": "1.0.0-rc.31",
  "description": "GraphQL introspection viewer",
  "main": "dist/voyager.lib.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "npm run check && npm run prettier:check",
    "start": "webpack-dev-server --config webpack-demo.config.js --hot --mode development",
    "clean:release": "rm -rf dist middleware",
    "clean:demo": "rm -rf demo-dist",
    "bundle:standalone": "webpack --mode=production --config webpack.config.js --mode production",
    "bundle:lib": "NODE_ENV=lib webpack --mode production --config webpack.config.js",
    "bundle": "npm run bundle:lib && npm run bundle:standalone",
    "compile:middleware": "tsc -d src/middleware/index.ts --outDir middleware --lib ES6,DOM,esnext.asynciterable",
    "build:release": "npm run bundle && npm run compile:middleware && npm run declarations",
    "build:demo": "npm run clean:demo && NODE_ENV=production webpack --mode production --config webpack-demo.config.js",
    "stats": "NODE_ENV=production webpack --json --mode production --config webpack.config.js > stats.json",
    "check": "tsc",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "declarations": "tsc --emitDeclarationOnly -p tsconfig.lib.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/commonmark": "0.27.5",
    "@types/lodash": "4.14.182",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "file-loader": "6.2.0",
    "graphql": "16.5.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "path-browserify": "^1.0.1",
    "postcss-cssnext": "3.1.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-variables-loader": "5.0.1",
    "prettier": "2.6.2",
    "raw-loader": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-svg-loader": "2.1.0",
    "style-loader": "0.23.1",
    "svg-as-symbol-loader": "1.2.2",
    "ts-loader": "5.3.3",
    "typescript": "4.7.2",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.1",
    "webpack-node-externals": "3.0.0"
  },
  "peerDependencies": {
    "graphql": ">=16.5.0",
    "react": ">=15.4.2",
    "react-dom": ">=15.4.2"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@f/animate": "^1.0.1",
    "@mui/material": "^5.8.1",
    "commonmark": "0.30.0",
    "lodash": "4.17.21",
    "svg-pan-zoom": "^3.6.1",
    "viz.js": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IvanGoncharov/graphql-voyager.git"
  }
}
